$PROBLEM SSE IV PK 1 CMT 100ID 2OBS
$INPUT ID TIME DV

$DATA data2.csv IGNORE=@

$PRED
 DOSE = 100
 CL   = THETA(1)*EXP(ETA(1))
 V    = THETA(2)*EXP(ETA(2))
 CONC = DOSE/V*EXP(-CL/V*TIME)
 Y    = LOG(CONC) + EPS(1)

$THETA (1e-10,1)     ; 1 TV.CL
$THETA (1e-10,1)     ; 2 TV.V
$OMEGA 0.1           ; 1 IIV.CL
$OMEGA 0.1           ; 2 IIV.V
$SIGMA 0.04          ; RESERR

$ESTIMATION MAXEVAL=9999 METHOD=1 INTERACTION
$COVARIANCE PRINT=E
